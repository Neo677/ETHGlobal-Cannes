# MetaCarTag Environment Configuration

# =============================================================================
# PRIVY AUTHENTICATION
# =============================================================================
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here

# =============================================================================
# BLOCKCHAIN CONFIGURATION
# =============================================================================
# Private key for contract deployment and transactions
PRIVATE_KEY=your_wallet_private_key_here

# Oasis Sapphire Network Configuration
NEXT_PUBLIC_SAPPHIRE_TESTNET_URL=https://testnet.sapphire.oasis.dev
NEXT_PUBLIC_SAPPHIRE_MAINNET_URL=https://sapphire.oasis.io
NEXT_PUBLIC_SAPPHIRE_TESTNET_CHAIN_ID=23295
NEXT_PUBLIC_SAPPHIRE_MAINNET_CHAIN_ID=23294

# Contract Addresses (deployed contracts)
NEXT_PUBLIC_CARTE_GRISE_NFT_ADDRESS=your_deployed_contract_address
NEXT_PUBLIC_CONFIDENTIAL_NFT_ADDRESS=your_deployed_contract_address

# =============================================================================
# SELF.ID CONFIGURATION
# =============================================================================
NEXT_PUBLIC_SELF_ID_CLIENT_URL=https://self.id
NEXT_PUBLIC_SELF_ID_NETWORK=testnet-clay

# =============================================================================
# IPFS CONFIGURATION
# =============================================================================
NEXT_PUBLIC_IPFS_GATEWAY=https://ipfs.io/ipfs/
NEXT_PUBLIC_WEB3_STORAGE_TOKEN=your_web3_storage_token

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
NEXT_PUBLIC_APP_NAME=MetaCarTag
NEXT_PUBLIC_APP_DESCRIPTION=Decentralized Vehicle Management
NEXT_PUBLIC_APP_URL=https://metacartag.com

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================
NODE_ENV=development
NEXT_PUBLIC_DEBUG_MODE=true

# =============================================================================
# ANALYTICS (Optional)
# =============================================================================
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_ga_id
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token 