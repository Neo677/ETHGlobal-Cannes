{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_PRIVY_APP_ID": "@next_public_privy_app_id",
    "NEXT_PUBLIC_SELF_CERAMIC_API_URL": "@next_public_self_ceramic_api_url"
  }
} 